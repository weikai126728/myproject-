	width: 0.726rem;
	height: 0.637rem;
	margin-top: 0.533rem;
	margin-right: 0.325rem;
}

.yjfk .fl {
	width: 0.666rem;
	height: 0.651rem;
	margin-top: 0.474rem;
	margin-right: 0.355rem;
}

.gfkf .fl {
	width: 0.607rem;
	height: 0.651rem;
	margin-top: 0.488rem;
	margin-right: 0.4rem;
}

.kfdh .fl {
	width: 0.666rem;
	height: 0.607rem;
	margin-top: 0.503rem;
	margin-right: 0.385rem;
}

.out {
	width: 100%;
	background-color: #FFFFFF;
	border: 1px solid #EBEBEB;
	border-left: none;
	border-right: none;
	margin-bottom: 2rem;
	-webkit-tap-highlight-color:transparent;
     			-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
        		tap-highlight-color: rgba(0, 0, 0, 0);
}

.out input {
	width: 100%;
	padding: 0.5rem 0;
	color: #4B4B4B;
	font-size: 0.6rem;
}

.bottom {
	width: 100%;
	height: 2.192rem;
	background-color: #FFFFFF;
	border-top: 1px solid #EBEBEB;
	text-align: center;
	position: fixed;
	bottom: 0;
	left: 0;
}

.bottom .sy, .bottom .wd {
	margin-left: 13%;
	font-size: 0.533rem;
	width: 1.2rem;
	margin-top: 0.355rem;
	text-align: center;
	-webkit-tap-highlight-color:transparent;
     			-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
        		tap-highlight-color: rgba(0, 0, 0, 0);
}

.bottom .sy a {
	color: #333333;
}

.bottom .sy img {
	width: 0.829rem;
	height: 0.74rem;
	margin: 0 auto;
	margin-bottom: 0.207rem;
}

.bottom .wd {
	margin-right: 13%;
}

.bottom .wd img {
	width: 0.711rem;
	height: 0.77rem;
	margin: 0 auto;
	margin-bottom: 0.207rem;
}

.bottom .sy, .bottom .wd {
	font-size: 0.5rem;
}

.bottom .wd {
	width: 7.5%;
}

.bottom .wd span {
	color: #f84f18;
}

.bottom .gw {
	width: 2.577rem;
	height: 2.577rem;
	margin-left: 21%;
	margin-top: -4%;
	-webkit-tap-highlight-color:transparent;
     			-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
        		tap-highlight-color: rgba(0, 0, 0, 0);
}

.bottom .gw img {
	width: 2.577rem;
	height: 2.577rem;
	-webkit-tap-highlight-color:transparent;
     			-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
        		tap-highlight-color: rgba(0, 0, 0, 0);
}
