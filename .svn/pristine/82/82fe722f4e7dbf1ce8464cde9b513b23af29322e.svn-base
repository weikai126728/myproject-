package com.abbot.schimneylife.pojo.weixin;

public class Activities {

	private Integer aid;
	private String coupon_stock_id;// 代金券批次id
	private String resp_count;// 返回记录数
	private String success_count;// 成功记录数
	private String openid;// 用户在商户appid下的唯一标识
	private String coupon_id;// 对一个用户成功发放代金券则返回代金券id
	private String sendtag;// 发放标记
	private String getTime;// 用户领取代金券的时间
	private Integer count;// 发放次数
	private String cardId;// 卡包id
	private Integer tag;// 使用标记
	private String cadeId;// 卡包ID
	private int coupon_value;// 代金券面值
	private int coupon_mininum;// 代金券门槛
	private String coupon_name;// 代金券名
	private String coupon_desc;// 代金券描述
	private String scope;// 全场
	private String send_source;// 正常
	private String beginTime;// 代金券开始时间
	private String endTime;// 代金券结束时间
	private String coupon_state;// 代金券状态：SENDED-可用，USED-已实扣，EXPIRED-已过期
	private String msg;

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}

	public String getCadeId() {
		return cadeId;
	}

	public void setCadeId(String cadeId) {
		this.cadeId = cadeId;
	}

	public int getCoupon_value() {
		return coupon_value;
	}

	public void setCoupon_value(int coupon_value) {
		this.coupon_value = coupon_value;
	}

	public int getCoupon_mininum() {
		return coupon_mininum;
	}

	public void setCoupon_mininum(int coupon_mininum) {
		this.coupon_mininum = coupon_mininum;
	}

	public String getCoupon_name() {
		return coupon_name;
	}

	public void setCoupon_name(String coupon_name) {
		this.coupon_name = coupon_name;
	}

	public String getCoupon_desc() {
		return coupon_desc;
	}

	public void setCoupon_desc(String coupon_desc) {
		this.coupon_desc = coupon_desc;
	}

	public String getScope() {
		return scope;
	}

	public void setScope(String scope) {
		this.scope = scope;
	}

	public String getSend_source() {
		return send_source;
	}

	public void setSend_source(String send_source) {
		this.send_source = send_source;
	}

	public String getBeginTime() {
		return beginTime;
	}

	public void setBeginTime(String beginTime) {
		this.beginTime = beginTime;
	}

	public String getEndTime() {
		if (endTime != null) {
			return endTime.substring(0, 19);
		} else {
			return null;
		}
	}

	public void setEndTime(String endTime) {
		this.endTime = endTime;
	}

	public String getCoupon_state() {
		return coupon_state;
	}

	public void setCoupon_state(String coupon_state) {
		this.coupon_state = coupon_state;
	}

	public Integer getTag() {
		return tag;
	}

	public void setTag(Integer tag) {
		this.tag = tag;
	}

	public String getCardId() {
		return cardId;
	}

	public void setCardId(String cardId) {
		this.cardId = cardId;
	}

	public String getSendtag() {
		return sendtag;
	}

	public void setSendtag(String sendtag) {
		this.sendtag = sendtag;
	}

	public Integer getCount() {
		return count;
	}

	public void setCount(Integer count) {
		this.count = count;
	}

	public String getGetTime() {
		return getTime;
	}

	public void setGetTime(String getTime) {
		this.getTime = getTime;
	}

	public Integer getAid() {
		return aid;
	}

	public void setAid(Integer aid) {
		this.aid = aid;
	}

	public String getCoupon_stock_id() {
		return coupon_stock_id;
	}

	public void setCoupon_stock_id(String coupon_stock_id) {
		this.coupon_stock_id = coupon_stock_id;
	}

	public String getResp_count() {
		return resp_count;
	}

	public void setResp_count(String resp_count) {
		this.resp_count = resp_count;
	}

	public String getSuccess_count() {
		return success_count;
	}

	public void setSuccess_count(String success_count) {
		this.success_count = success_count;
	}

	public String getOpenid() {
		return openid;
	}

	public void setOpenid(String openid) {
		this.openid = openid;
	}

	public String getCoupon_id() {
		return coupon_id;
	}

	public void setCoupon_id(String coupon_id) {
		this.coupon_id = coupon_id;
	}

}
