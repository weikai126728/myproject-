/*浜у搧浠嬬粛!*/
.cp{
	text-align: center;
	margin-bottom: 20px;
}
.cp_bottom{
	margin-left: 20px;
}
.cp_bottom ul li{
	float: left;
	height: 150px;
	margin-right: 2px;
	margin-bottom: 2px;
	position: relative;
}
.cp_bottom ul .li1,.cp_bottom ul li .s1{
	width: 270px;
	transition: 1s;
}
.cp_bottom ul .li2,.cp_bottom ul li .s2{
	width: 210px;
}
.cp_bottom ul .li3,.cp_bottom ul li .s3{
	width: 150px;
}

.cp_bottom ul li div{
	height: 150px;
	position: absolute;
	left: 0;
	bottom: 150px;
	opacity: 0;
	background-color: #000000;
}

.cp_bottom ul li a img{
	height: 150px;
	border: 1px solid #CCCCCC;
}
.cp_bottom ul li a .img1{
	width: 270px;
}
.cp_bottom ul li a .img2{
	width: 210px;
}
.cp_bottom ul li a .img3{
	width: 150px;
}

.cp_bottom ul .li1:hover .s1{
	bottom: 0;
	opacity: 1;
}


/*鏂伴椈&鍔ㄦ��&鍜ㄨ!*/

.new_box{
