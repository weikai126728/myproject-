
body{
	background-color: #F4F4F4;
}
.top_f{
	width: 100%;
	position: fixed;
	top: 0;
	z-index: 999;
}
.top{
	width: 100%;
	text-align: center;
	height: 1.95rem;
	background-color: #FFFFFF;
	border-bottom: 0.266rem solid #F4F4F4;

}
.top .fl{
	width: 0.385rem;
	height: 0.696rem;
	margin-left: 0.725rem;
	margin-top: 0.637rem;
}
.top .fl img{
	width: 100%;
}
.top span{
	line-height: 1.95rem;
	font-size: 0.681rem;
	margin-left: -0.725rem;
	color: #4B4B4B;
	/*padding: 0.622rem 0;
	display: inline-block;*/
}
.dingdan_lb{
	margin-top: 2.266rem;
}


.no {
	width: 100%;
	height: 100%;
	text-align: center;
	margin-top: 8rem;
}
.no ul{
	margin: auto;
}
.no li img{
	width: 3.629rem;
	height: 2.933rem;
	margin: auto;
	margin-bottom: 1rem;
}
.no li a{
	padding: 0 0.888rem;
	font-size: 0.65rem;
	border: 1px solid #E7690D;
	color: #E7690D;
	height: 1.155rem;
	line-height: 1.155rem;
	display: inline-block;
	border-radius: 0.6rem;
	-webkit-tap-highlight-color:transparent;
     			-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
        		tap-highlight-color: rgba(0, 0, 0, 0);
}
.top_f .dingdan_nav{
	background-color: #FFFFFF;
	width: 100%;
	border-bottom: 1px solid #EBEBEB;

}
.top_f .dingdan_nav li{
	float: left;
	margin-left: 0.92rem;
}
.top_f .dingdan_nav li a{
	font-size: 0.6rem;
	padding: 0.8rem 0.207rem;
	display: inline-block;
	color: #666666;
}
.top_f .dingdan_nav li .active{
	border-bottom: 2px solid #EE3900;
}

.dingdan_nr{
	margin-top: 5rem;
}

.dingdan_box{
	background-color: #FFFFFF;
	margin-top: 0.296rem;
	padding-bottom: 0.592rem;
}
.dingdan_box .dingdan_box_top{
	font-size: 0.6rem;
	height: 1.481rem;
	border-bottom: 1px solid #EBEBEB;
}
.dingdan_box .dingdan_box_top .fl{
	margin-left: 0.592rem;
	color: #444444;
	line-height: 1.481rem;
	margin-top: 0.05rem;
}
.dingdan_box .dingdan_box_top .fr{
	margin-right: 0.592rem;
	color: #EE3900;
	line-height: 1.481rem;
}


.dingdan_box_bottom{
	display: block;
	margin-left: 0.592rem;
	margin-right: 0.592rem;
	padding-bottom: 0.444rem;
	margin-top: 0.444rem;
	border-bottom: 1px solid #EBEBEB;
}
.dingdan_box_bottom img{
	width: 4rem;
	height: 4rem;
}
.dingdan_box_bottom ul{
	width: 9.63rem;
}
.dingdan_box_bottom .fl .li_title{
	font-size: 0.6rem;
	color: #333333;
	margin-bottom: 0.3rem;
	width: 10.5rem;
}
.dingdan_box_bottom .fl .li_title2{
	font-size: 0.55rem;
	color: #989898;
	margin-bottom: 0.5rem;
}
.dingdan_box_bottom .fl .li_jiage{
	width: 100%;
}
.dingdan_box_bottom .fl .li_jiage .fl{
	font-size: 0.711rem;
	color: #EE3900;
	line-height: 1rem;
}
.dingdan_box_bottom .fl .li_jiage .fl em{
	font-size: 0.444rem;
}
.dingdan_box_bottom .fl .li_jiage .fr{
	font-size: 0.6rem;
	color: #cccccc;
	line-height: 1rem;
	font-weight: normal;
}

.dingdan_bottom span{
	font-size: 0.562rem;
	color: #333333;
	line-height: 1.733rem;
	margin-right: 0.592rem;
}
.leixing div{
	margin-right: 0.592rem;
}
.leixing div a{
	font-size: 0.6rem;
	color: #4B4B4B;
	padding: 0 0.503rem;
	padding-top: 0.1rem;
	display: inline-block;
	height: 1.185rem;
	line-height: 1.185rem;
	border: 1px solid #EBEBEB;
	border-radius: 0.2rem;
	margin-left: 0.296rem;
}



















