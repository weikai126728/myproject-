.fc{
	width: 100%;
	height: 100%;
	z-index: 2;
	position: fixed;
	top: 0;
	display: none;
	-webkit-tap-highlight-color:transparent;
     			-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
        		tap-highlight-color: rgba(0, 0, 0, 0);
}
.zhifu_tanchu{
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,0.5);
	-webkit-tap-highlight-color:transparent;
     			-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
        		tap-highlight-color: rgba(0, 0, 0, 0);
}
.zhifutanchu_box{
	width: 100%;
	height: 2.162rem;
