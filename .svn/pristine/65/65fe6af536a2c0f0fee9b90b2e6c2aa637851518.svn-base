

body{
	background-color: #F0F0F0;
}
.top{
	text-align: center;
	margin-bottom: 0.296rem;
	background-color: #FFFFFF;
	width: 100%;
	height: 1.955rem;
}
.top .fl{
	width: 0.385rem;
	height: 0.692rem;
}
.top .fl img{
	width: 100%;
	margin-left: 0.725rem;
	margin-top: 0.637rem;
}
.top span{
	font-size: 0.711rem;
	line-height: 1.955rem;
}


.xg li{
	width: 100%;
	height: 2.144rem;
	font-size: 0.6rem;
	background-color: #FFFFFF;
	border: 1px solid #EBEBEB;
	border-left: none;
	border-right: none;
	margin-bottom: 0.296rem;
	-webkit-tap-highlight-color:transparent;
     			-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
        		tap-highlight-color: rgba(0, 0, 0, 0);
}
.xg li i{
	display: block;
	width: 100%;
	text-align: center;
	line-height: 2.144rem;
	font-size: 0.6rem;
}
.xg li i span{
	color: #4B4B4B;
	margin-left: 0.5rem;
}
.xg li i em{
	color: #000000;
	margin-left: 6rem;
	vertical-align: middle;
}
.xg li i .fr{
	width: 2.4%;
	margin-top: 0.8rem;
	vertical-align: middle;
	margin-right: 0.5rem;
}














